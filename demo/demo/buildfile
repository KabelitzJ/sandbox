dependencies =
import dependencies += libsbx-utility%liba{sbx_utility}
import dependencies += libsbx-core%liba{sbx_core}
import dependencies += libsbx-io%liba{sbx_io}
import dependencies += libsbx-scripting%liba{sbx_scripting}
import dependencies += libsbx-math%liba{sbx_math}
import dependencies += libsbx-devices%liba{sbx_devices}
import dependencies += libsbx-graphics%liba{sbx_graphics}

exe{demo}: {hxx ixx txx cxx}{**} $dependencies

cxx.poptions += "-I$out_root" "-I$src_root"
